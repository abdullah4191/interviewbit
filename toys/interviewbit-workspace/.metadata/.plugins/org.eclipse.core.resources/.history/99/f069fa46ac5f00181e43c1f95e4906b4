package codeScratch;

public class SqrtSolution {
	
	public int sqrt(int a) {
		for(int i=0;i<a/2;i++){
	        if(a/i==i)return i;
	    }
		return 1;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		SqrtSolution instance = new SqrtSolution();
		System.out.println(SqrtSolution.);
	}

}
